<+>/* Set the background color for the entire application to a dark color */\r\n.root {\r\n    -fx-background-color: #89fd03; /* Dark gray background */\r\n    -fx-background-image: url('whatsup.jpg');\r\n\r\n}\r\n.text {\r\n    /*-fx-fill: black; !* Text color *!*/\r\n    -fx-font-family: \"Times New Roman\"; /* Use Arial font (replace with your desired font) */\r\n\r\n\r\n}\r\n/* Style the window title text */\r\n.root .root-title .window .labels-text  {\r\n    -fx-text-fill: white; /* Title text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n    -fx-font-weight: bold; /* Bold font */\r\n}\r\n\r\n/* Style the header text */\r\n\r\n/* Style the TabPane */\r\n.tab-pane {\r\n    -fx-background-color: #292929; /* Dark gray background color */\r\n    -fx-text-background-color: white; /* Tab text color */\r\n    -fx-tab-min-height: 30;\r\n    -fx-tab-min-width: 50;\r\n}\r\n\r\n/* Style the tabs */\r\n.tab {\r\n    -fx-background-color: #99FFFF; /* Dark gray tab background color */\r\n    -fx-text-fill: #FFD700; /* Bright yellow tab text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n    -fx-font-weight: bold; /* Bold font */\r\n}\r\n\r\n/* Style the buttons */\r\n.button {\r\n    -fx-background-color: #15913C; /* Darker green button background */\r\n    -fx-text-fill: white; /* Button text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n}\r\n\r\n/* Style the text fields */\r\n.text-field {\r\n    -fx-background-color: #333333; /* Dark gray background */\r\n    -fx-text-fill: white; /* Text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n}\r\n\r\n/* Style the text area */\r\n.text-area {\r\n    -fx-background-color: #333333; /* Dark gray background */\r\n    -fx-text-fill: white; /* Text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n}\r\n\r\n/* Style the ListView */\r\n.list-view {\r\n    -fx-background-color: #292929; /* Dark gray background color */\r\n    -fx-text-background-color: white; /* ListView text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n}\r\n\r\n/* Style the ListView items */\r\n.list-cell {\r\n    -fx-background-color: transparent; /* Make items transparent */\r\n    -fx-text-fill: white; /* ListView item text color */\r\n    -fx-font-family: \"Arial\"; /* Use Arial font (replace with your desired font) */\r\n}\r\n\r\n#punch {\r\n    -fx-font-family: \"Times New Roman\";\r\n    -fx-font-size: 20;\r\n    -fx-text-shadow: 5px 5px;\r\n    -fx-text-fill: white ;\r\n}\r\n\r\n.my-buttons{\r\n    -fx-font-size: 18;\r\n    -fx-text-fill: #f8f8f8;\r\n}\r\n\r\n#header{\r\n    -fx-text-fill: white ;\r\n}\r\n\r\n.labels-newUserUI\r\n{\r\n    -fx-text-fill: white;\r\n    -fx-font-size: 14;\r\n}\r\n
